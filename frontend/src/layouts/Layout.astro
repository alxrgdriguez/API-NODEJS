---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
</head>
<body>
<Header/>
<slot/>
<Footer/>
</body>
</html>
